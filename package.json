    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "exports-loader": "^3.1.0",
    "imports-loader": "^3.1.1",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.3",
    "postcss": "^8.4.12",
    "prettier": "2.6.2",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
